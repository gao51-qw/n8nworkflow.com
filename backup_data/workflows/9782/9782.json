          "type": "@n8n/n8n-nodes-langchain.outputParserStructured",
          "position": [
            2048,
            592
          ],
          "parameters": {
            "jsonSchemaExample": "[\n  {\n    \"label\": \"Billing\",\n    \"count\": 8,\n    \"examples\": [\n      \"Unable to update billing info due to error 502\",\n      \"Invoice shows wrong amount\"\n    ]\n  }\n]"
          },
          "typeVersion": 1.3
