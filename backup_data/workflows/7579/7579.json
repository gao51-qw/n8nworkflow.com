            -624
          ],
          "parameters": {
            "modelId": {
              "__rl": true,
              "mode": "list",
              "value": "gpt-4o-mini",
              "cachedResultName": "GPT-4O-MINI"
            },
            "options": {},
            "messages": {
              "values": [
                {
                  "content": "={{$json.prompt}}\n"
                }
              ]
            }
          },
          "credentials": {
            "openAiApi": {
              "id": "oETjE0hiLJ8SZxWX",
              "name": "YOUR_API_KEY"
            }
          },
          "retryOnFail": true,
          "typeVersion": 1.8,
          "waitBetweenTries": 2000
        },
        {
          "id": "fb93ada6-17e2-46c8-8ca3-b24efcd67c97",
          "name": "Extract AI Output",
          "type": "n8n-nodes-base.set",
          "position": [
