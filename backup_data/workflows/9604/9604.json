            176,
            0
          ],
          "parameters": {
            "options": {},
            "conditions": {
              "options": {
                "version": 2,
                "leftValue": "",
                "caseSensitive": true,
                "typeValidation": "strict"
              },
              "combinator": "and",
              "conditions": [
                {
                  "id": "e62d1e04-38f2-454f-9301-6b6b896a9e5a",
                  "operator": {
                    "type": "boolean",
                    "operation": "true",
                    "singleValue": true
