            "method": "POST",
            "options": {},
            "jsonParameters": true,
            "bodyParametersJson": "{\n  \"email\": \"{{$json[\"email\"]}}\",\n  \"tags\": [\"Customer\"]\n}"
          },
          "credentials": {
            "httpBasicAuth": {
              "user": "your_crm_api_user",
              "password": "your_crm_api_key"
            }
          },
