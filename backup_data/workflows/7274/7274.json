        {
          "id": "b8e80d8b-5f75-48a6-adda-3dc1a39e8f3f",
          "name": "Lab Report Email Trigger",
          "type": "n8n-nodes-base.emailReadImap",
          "position": [
            -600,
            360
          ],
          "parameters": {
