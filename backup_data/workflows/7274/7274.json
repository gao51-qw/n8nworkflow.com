        {
          "id": "09a54768-7021-4d1e-abe8-2d4dd57920f5",
          "name": "Medical AI Model",
          "type": "@n8n/n8n-nodes-langchain.lmChatOllama",
          "position": [
            -72,
            580
          ],
          "parameters": {
            "model": "llama3.2-16000:latest",
            "options": {}
          },
          "credentials": {
            "ollamaApi": {
              "id": "credential-id",
