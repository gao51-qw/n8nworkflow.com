          "id": "2fbe1f25-d09c-43da-933e-6903c6b2155b",
          "name": "HTTP Request",
          "type": "n8n-nodes-base.httpRequest",
          "position": [
            208,
            0
          ],
          "parameters": {
            "url": "=https://api.etherscan.io/v2/api?chainid=1&module=account&action=tokentx&address=YOUR_WALLET_HERE&sort=asc&apikey=YOUR_KEY_HERE",
            "method": "=GET",
