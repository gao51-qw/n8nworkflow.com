        {
          "id": "4ed6aade-7953-401e-9c87-54f159b81428",
          "name": "Daily Notice Check - 9 AM",
          "type": "n8n-nodes-base.scheduleTrigger",
          "position": [
            -500,
            400
          ],
          "parameters": {
            "rule": {
              "interval": [
