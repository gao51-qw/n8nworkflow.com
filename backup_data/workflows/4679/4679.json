          "type": "n8n-nodes-base.scheduleTrigger",
          "position": [
            -140,
            160
          ],
          "parameters": {
            "rule": {
              "interval": [
                {
                  "triggerAtHour": 4
                }
              ]
            }
          },
          "typeVersion": 1.2
        },
        {
          "id": "84a83cf9-63fc-40cf-bfa6-bc01ad67b3b7",
          "name": "Fetch Gmail Inbox Emails",
          "type": "n8n-nodes-base.gmail",
          "position": [
            80,
