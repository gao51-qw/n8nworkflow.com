          "name": "Keep meetings with external attendees",
          "type": "n8n-nodes-base.filter",
          "position": [
            1040,
            0
          ],
          "parameters": {
            "options": {},
            "conditions": {
              "options": {
                "version": 2,
                "leftValue": "",
