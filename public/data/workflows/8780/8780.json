        {
          "id": "acbfe599-0e66-43f8-acf8-92f3d863f963",
          "name": "Re-Access Binary Data from Previous Node",
          "type": "n8n-nodes-base.code",
          "notes": "¬© 2025 Lucas Peyrin",
          "position": [
            704,
