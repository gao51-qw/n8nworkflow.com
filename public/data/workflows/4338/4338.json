          ],
          "parameters": {
            "jsCode": "return items.map(item => {\n  // Sonderzeichen entfernen und formatieren\n  const cleanName = item.json.name\n    .replace(/[^\\w\\s-]/g, '')\n    .replace(/\\s+/g, '_')\n    .substring(0, 50); // Maximale L√§nge begrenzen\n\n  const fileName = `${cleanName || 'backup'}.json`;\n  const jsonData = JSON.stringify(item.json);\n  \n  // WICHTIG: Setze den Dateinamen auch im JSON f√ºr den Pfad-Zugriff\n  item.json.fileName = fileName;\n\n  item.binary = {\n    data: {\n      data: Buffer.from(jsonData).toString('base64'),\n      mimeType: 'application/json',\n      fileName: fileName\n    }\n  };\n  return item;\n});"
          },
