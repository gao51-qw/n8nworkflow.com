      "nodes": [
        {
          "id": "a244c890-bc5f-4edd-91f4-37192d7807c2",
          "name": "Daily Market News Trigger",
          "type": "n8n-nodes-base.scheduleTrigger",
          "position": [
            -3000,
            660
          ],
          "parameters": {
            "rule": {
              "interval": [
                {
                  "field": "cronExpression",
                  "expression": "15 9 * * 1-5"
