            -2340,
            660
          ],
          "parameters": {
            "url": "https://finnhub.io/api/v1/company-news",
            "options": {},
            "sendQuery": true,
            "authentication": "genericCredentialType",
            "genericAuthType": "httpHeaderAuth",
            "queryParameters": {
              "parameters": [
