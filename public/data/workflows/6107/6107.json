          ],
          "parameters": {
            "functionCode": "// expects a message like ‚Äú/rest Tahrir‚Äù\nconst txt = $json.message?.text || '';\nconst area = txt.replace(/^(\\/rest\\s*)/i, '').trim() || 'Cairo';\nreturn [{ area }];"
          },
