        },
        {
          "id": "store-entry",
          "name": "Store Review Entry",
          "type": "n8n-nodes-base.set",
          "notes": "Save processed entry",
          "position": [
            1650,
            300
          ],
          "parameters": {
            "values": {
              "string": [
                {
                  "name": "reviewEntry",
