            300
          ],
          "parameters": {
            "limit": 50,
            "query": "={{ $json.topic }}",
            "yearTo": "={{ $json.yearTo }}",
            "resource": "academic",
            "yearFrom": "={{ $json.yearFrom }}",
            "operation": "search",
            "providers": [
              "pubmed",
              "semantic-scholar",
              "arxiv"
            ],
            "additionalFields": {
              "fields": [
