        {
          "id": "0377e81f-4ede-491c-acb2-960d57c884bf",
          "name": "Format list of events and sender mobile num",
          "type": "n8n-nodes-base.code",
          "position": [
            720,
            240
          ],
          "parameters": {
            "jsCode": "// Pull in *all* today's events and build your string:\nconst events = $input.all();\nconst list = events\n  .map(evt => {\n    const time = new Date(evt.json.start.dateTime)\n  .toLocaleTimeString('en-US', {\n    timeZone: evt.json.start.timeZone,\n    hour:     '2-digit',\n    minute:   '2-digit'\n  });\n    return `‚Ä¢ ${evt.json.summary} at ${time}`;\n  })\n  .join(\"\\n\");\n\nconst output = {\n  json: {\n    // send to this user's number\n    to: \"+1234567890\",\n    eventsList: list || \"No events today.\",\n    // name of user\n    calendar: \"Anne\"\n  }\n}\n\nreturn output;"
          },
          "typeVersion": 2
        },
