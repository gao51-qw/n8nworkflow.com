                  "id": "0630be0c-738a-4f1b-b782-67de592305dc",
                  "name": "id",
                  "type": "string",
                  "value": "={{ $json.id }}"
                },
                {
                  "id": "71be8325-4dc1-4567-972a-74ee46c7fe50",
