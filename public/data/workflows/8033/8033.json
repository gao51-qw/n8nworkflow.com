          "id": "fetch-youtube-rss",
          "name": "Fetch YouTube RSS",
          "type": "n8n-nodes-base.httpRequest",
          "position": [
            400,
            300
          ],
          "parameters": {
            "url": "https://www.youtube.com/feeds/videos.xml?channel_id=YOUR_CHANNEL_ID_HERE",
